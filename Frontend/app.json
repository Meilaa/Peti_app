{
  "expo": {
    "name": "Peti",
    "slug": "Peti",
    "scheme": "Peti",
    "version": "1.0.0",
    "icon": "./assets/images/main-logo.png",
    "newArchEnabled": true,
    "android": {
      "package": "com.peti.app",
      "permissions": [
        "CAMERA"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC2Mdcqzerqps6gEXK38l2IwyBkswD7mgo"
        }
      }
    },
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "We need access to your camera to scan barcodes.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "Peti.lt"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1e48fb91-198e-42c8-bcb7-dc23bdda2ace"
      }
    }
  }
}
